<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="277.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cuchaz.jfxgl.demo.popups.MainController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <AnchorPane prefHeight="246.0" prefWidth="360.0" BorderPane.alignment="CENTER">
         <children>
            <ChoiceBox fx:id="choice" layoutX="14.0" layoutY="14.0" prefWidth="150.0" />
            <ColorPicker layoutX="188.0" layoutY="14.0" prefHeight="26.0" prefWidth="158.0" />
            <DatePicker layoutX="188.0" layoutY="66.0" prefHeight="26.0" prefWidth="158.0" />
            <Button fx:id="dialogButton" layoutX="14.0" layoutY="66.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="150.0" text="Show a dialog" />
            <Button fx:id="pickFileButton" layoutX="14.0" layoutY="115.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="150.0" text="Pick a file" />
            <Button fx:id="pickDirButton" layoutX="192.0" layoutY="162.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="150.0" text="Pick a folder" />
            <Button fx:id="saveFileButton" layoutX="14.0" layoutY="162.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="150.0" text="Save a file" />
            <Button fx:id="pickFilesButton" layoutX="192.0" layoutY="115.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="150.0" text="Pick somes files" />
            <ComboBox fx:id="combo" layoutX="14.0" layoutY="207.0" prefWidth="150.0" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
